#divUserCalendar { height:410px; min-height:400px;height: auto !important;}
#divUserCalendar a {color: #FFC}

#divUserCalendar p {padding: 15px 0; text-align: justify}

#divUserCalendar h2 { color: #616D7E; font-weight: normal; letter-spacing: .15em}

/* navigation*/

#divUserCalendar table {
	border-collapse: separate;
	border: 1px solid #9DABCE;
	border-width: 0px 0px 1px 1px;
	margin: 10px auto;
	font-size: 20px;
}
#divUserCalendar td, th {
	width: 50px;
	height: 45px;
	/*text-align: center;*/
	vertical-align: top;
        font-size: 14px;
	color: #444;
	position: relative;
	border: 1px solid #C8BBBE;
}
#divUserCalendar th {
	height: 25px;
	font-weight: bold;
	font-size: 14px;
}
#divUserCalendar td:hover, th:hover {
	background-color:#AAA;
	color: #222;
}
#divUserCalendar td.date_has_event {
	background-color:#CFECEC;
	color: #566D7E;
	cursor:pointer;
}
#divUserCalendar td.date_has_event:hover {
	background-color:#87AFC7;
	color: white;
	cursor:pointer;
}
